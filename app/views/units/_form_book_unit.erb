<div class="container">
  <div class="form-wrapper">

<%= simple_form_for([@unit, @booking]) do |f| %>
  <%= f.input :target_name %>
  <%= f.input :target_address %>
  <%= f.input :start_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.submit "Book Unit", class: "btn btn-primary" %>
<% end %>
  </div>
</div>
